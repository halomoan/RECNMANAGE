<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="fin.re.conmgmts1.controller.Home"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<Page
		showHeader="false"
		enableScrolling="false" >
		<TileContainer
			id="container"
			tileDelete="handleTileDelete"
			tiles="{/ZTileInfoSet}">
			<StandardTile
				press="onTilePress"
				icon="{icon}"
				type="{type}"
				number="{number}"
				numberUnit="{numberUnit}"
				title="{title}"
				info="{info}"
				infoState="{infoState}">
				<customData>
    				<core:CustomData key="bukrs" value="{bukrs}" writeToDom="false"/>
    				<core:CustomData key="swenr" value="{swenr}" writeToDom="false"/>
    				<core:CustomData key="recntype" value="{recntype}" writeToDom="false"/>
    				<core:CustomData key="title" value="{title}" writeToDom="false"/>
    				<core:CustomData key="info" value="{info}" writeToDom="false"/>
    			</customData>
			</StandardTile>
		</TileContainer>
	</Page>
</mvc:View>