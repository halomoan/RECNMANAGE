<core:FragmentDefinition
        xmlns="sap.m"
        xmlns:core="sap.ui.core"
        xmlns:smartTable="sap.ui.comp.smarttable">
    <Dialog title="{i18n>Terms.BreakOption.Simulation.Heading}">
        <smartTable:SmartTable
                id="simulated-break-options"
                useTablePersonalisation="true"
                tableType="ResponsiveTable"
                tableBindingPath="NtSim"
                editable="false"
                entitySet="ContractNtSimDataSet"
                useExportToExcel="false"
                enableAutoBinding="true">
        </smartTable:SmartTable>
        <buttons>
            <Button id="close-break-option-simulation" text="{i18n>Common.Dialog.Close}" press="close"/>
        </buttons>
    </Dialog>
</core:FragmentDefinition>
