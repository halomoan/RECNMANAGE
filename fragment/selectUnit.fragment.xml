<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<TableSelectDialog
		noDataText="No Products Found"
		title="Select Product"
		search="onUnitSearch"
		confirm="handleClose"
		cancel="handleClose"
		items="{
			path : '/RentalObjectSearchSet',
			sorter : {
				path : 'Xmetxt',
				descending : false
			},
			filters: [
            	{path: 'Bukrs',operator: 'EQ',value1: '1001'},
            	{path: 'Swenr',operator: 'EQ',value1: '1001'}
        	]
		}">
		<ColumnListItem>
			<cells>
				<ObjectIdentifier
					title="{Smenr}"
					 />
				<ObjectIdentifier
					title="{Xmetxt}"
					 />
				<ObjectIdentifier
					title="{Rotype}"
					 />
			</cells>
		</ColumnListItem>
		<columns>
			<Column>
				<header>
					<Text text="ID" />
				</header>
			</Column>
			<Column
				demandPopin="true" >
				<header>
					<Text text="Unit" />
				</header>
			</Column>
			<Column
				demandPopin="true"
				hAlign="End">
				<header>
					<Text text="Unit Type" />
				</header>
			</Column>
		</columns>
	</TableSelectDialog>
</core:FragmentDefinition>