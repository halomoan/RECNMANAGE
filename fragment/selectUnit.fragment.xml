<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<TableSelectDialog
		growingThreshold="20"
		noDataText="No Products Found"
		title="Select Product"
		search="onUnitSearch"
		confirm="onUnitSelect"
		cancel="onUnitCancel"
		items="{
			path : '/RentalObjectSearchSet',
			filters: [
            	{path: 'Bukrs',operator: 'EQ',value1: '1001'},
            	{path: 'Swenr',operator: 'EQ',value1: '1001'},
            	{path: 'Rotype',operator: 'EQ',value1: 'RU'}
        	],
        	startIndex : 0, 
    		top : 3000      
		}">
		<ColumnListItem>
			<cells>
				<ObjectIdentifier
					title="{Smenr}"
					 />
				<ObjectIdentifier
					title="{Xmetxt}"
					 />
				<ObjectNumber
					number="{
						path: 'ROSize',
						formatter: '.formatter.numberUnit'
					}"
					unit="{ROUnit}"
					state="Success" />
				<ObjectIdentifier
					title="{Rotype}"
					 /></cells>
		</ColumnListItem>
		<columns>
			<Column>
				<header>
					<Text text="ID" />
				</header>
			</Column>
			<Column
				demandPopin="true" >
				<header>
					<Text text="Unit" />
				</header>
			</Column>
			<Column hAlign="End"
				demandPopin="true" >
				<header>
					<Text text="Size" />
				</header>
			</Column>
			<Column
				demandPopin="true"
				hAlign="End">
				<header>
					<Text text="Unit Type" />
				</header>
			</Column>
		</columns>
	</TableSelectDialog>
</core:FragmentDefinition>