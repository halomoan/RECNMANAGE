<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:f="sap.ui.layout.form"
	xmlns:l="sap.ui.layout"
	xmlns:core="sap.ui.core">
	<Popover
		showHeader="false"
		contentWidth="500px"
		contentHeight="420px"
		placement="Bottom" >
		<NavContainer
			id="ufNavCon">
			<Page
				id="ufmaster"
				title="{i18n>Dialog.Manage.UserFields}">
				<HBox class="sapUiSmallMargin">
					<f:SimpleForm
						editable="true"
						layout="ResponsiveGridLayout"
						labelSpanXL="4"
						labelSpanL="3"
						labelSpanM="4"
						labelSpanS="12"
						adjustLabelSpan="false"
						emptySpanXL="0"
						emptySpanL="4"
						emptySpanM="0"
						emptySpanS="0"
						columnsXL="2"
						columnsL="1"
						columnsM="1"
						columnsS="2"
						singleContainerFullSize="false"  >
						<f:content>
							<core:Title text="Dates" />
							<Label text="{i18n>UF.LeaseStartDate}" />
							<DatePicker value="{viewModel>/usrfields/zzhdate}" valueFormat="yyyy-MM-dd"/>
							 
							<Label text="{i18n>UF.1stStampingDate}"/>
							<DatePicker value="{viewModel>/usrfields/zzstmdate1}" valueFormat="yyyy-MM-dd"/>
							
							<Label text="{i18n>UF.2ndStampingDate}"/>
							<DatePicker value="{viewModel>/usrfields/zzstmdate2}" valueFormat="yyyy-MM-dd"/>
							
							<Label text="{i18n>UF.3rdStampingDate}"/>
							<DatePicker value="{viewModel>/usrfields/zzstmdate3}" valueFormat="yyyy-MM-dd"/>
							
							<Label text="{i18n>UF.NovationDate}"/>
							<DatePicker value="{viewModel>/usrfields/zznovdate}" valueFormat="yyyy-MM-dd"/>
							
							<Label text="{i18n>UF.DateOfPartialSurr}"/>
							<DatePicker value="{viewModel>/usrfields/zzpartdate}" valueFormat="yyyy-MM-dd"/>
							
							<Label text="{i18n>UF.BGExpirationDate}"/>
							<DatePicker value="{viewModel>/usrfields/zzbgedate}" valueFormat="yyyy-MM-dd"/>
						</f:content>
					</f:SimpleForm>	
						<f:SimpleForm
						editable="true"
						layout="ResponsiveGridLayout"
						labelSpanXL="4"
						labelSpanL="3"
						labelSpanM="4"
						labelSpanS="12"
						adjustLabelSpan="false"
						emptySpanXL="0"
						emptySpanL="4"
						emptySpanM="0"
						emptySpanS="0"
						columnsXL="2"
						columnsL="1"
						columnsM="1"
						columnsS="2"
						singleContainerFullSize="false"  >
						<f:content>
							<core:Title text="Misc" />
		                    <Label text="{i18n>UF.NoOfMonthsSecD}"/>   
		                    <Input value="{viewModel>/usrfields/zzmoncsh}"/>
		                     <Label text="{i18n>UF.NoOfMonthsBG}"/>   
		                    <Input value="{viewModel>/usrfields/zzmonbg}"/>
		                     <Label text="{i18n>UF.DGCG}"/>   
		                    <Input value="{viewModel>/usrfields/zzdgcg}"/>
		                    <Label text="{i18n>UF.LeaseTermYear}"/>   
		                    <Input value="{viewModel>/usrfields/zzltermy}"/>
		                    <Label text="{i18n>UF.LeaseTermMonth}"/>   
		                    <Input value="{viewModel>/usrfields/zzltermm}"/>
		                    <Label text="{i18n>UF.LeaseTermDay}"/>   
		                    <Input value="{viewModel>/usrfields/zzltermd}"/>
		                    <Label text="{i18n>UF.OptionToRenew}"/>   
		                    <Input value="{viewModel>/usrfields/zzoptrnew}"/>
		                    <Label text="{i18n>UF.IRASUsageCategory}"/>   
		                    <Input value="{viewModel>/usrfields/zzirasusg}"/>
		                    <Label text="{i18n>UF.POReference}"/>   
		                    <Input value="{viewModel>/usrfields/zzporef}"/>
		                    <Label text="{i18n>UF.AgentCommissionA}"/>   
		                    <Input value="{viewModel>/usrfields/zzagtcomm}"/>
		                     <Label text="{i18n>UF.RemarkForIncentive}"/>   
		                    <Input value="{viewModel>/usrfields/zzincvrem}"/>
						</f:content>
					</f:SimpleForm>	
				</HBox>		
				<footer>
					<Toolbar>
						<ToolbarSpacer/>
						<Button
							text="{i18n>Common.Dialog.Close}"
							press="onUserFieldsClose" />
					</Toolbar>
				</footer>
			</Page>
		</NavContainer>
	</Popover>
</core:FragmentDefinition>